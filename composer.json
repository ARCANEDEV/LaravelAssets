{
    "name":        "arcanedev/laravel-assets",
    "description": "This package allows you to manage multiple assets",
    "keywords":    ["arcanedev", "arcanesoft", "laravel", "assets", "laravel-mix", "yarn", "npm"],
    "homepage":    "https://github.com/ARCANEDEV/LaravelAssets",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=7.1.3",
        "ext-json":          "*",
        "arcanedev/support": "~4.4.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.7.0",
        "phpunit/phpunit":     "~7.0",
        "phpunit/phpcov":      "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Assets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Assets\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanedev\\Assets\\AssetsServiceProvider"
            ]
        }
    }
}
